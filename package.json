{
  "name": "nodejs-api-boilerplate",
  "author": "Emanuel Quimper <quimperemanuel@gmail.com> <https://github.com/EQuimper>",
  "repository": {
    "type": "git",
    "url": "https://github.com/EQuimper/nodejs-api-boilerplate"
  },
  "bugs": {
    "url": "https://github.com/EQuimper/nodejs-api-boilerplate/issues"
  },
  "engines": {
    "node": ">= 6",
    "npm": ">= 3"
  },
  "keywords": [
    "express",
    "es6",
    "es7",
    "rest",
    "api",
    "boilerplate",
    "mongo",
    "node",
    "javascript"
  ],
  "license": "MIT",
  "version": "0.0.1",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon dist",
    "dev:debug": "cross-env NODE_ENV=development MONGOOSE_DEBUG=true DEBUG=express:* nodemon dist",
    "dev:watch": "babel -w --out-dir=dist ./src",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src",
    "build": "yarn run clean && build",
    "clean": "rimraf dist"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.4.0",
    "cross-env": "^4.0.0",
    "eslint": "^3.19.0",
    "eslint-config-equimper": "^1.6.3",
    "morgan": "^1.8.1",
    "nodemon": "^1.11.0",
    "rimraf": "^2.6.1"
  },
  "dependencies": {
    "bcrypt": "^1.0.2",
    "body-parser": "^1.17.1",
    "compression": "^1.6.2",
    "dotenv": "^4.0.0",
    "express": "^4.15.2",
    "jsonwebtoken": "^7.3.0",
    "mongoose": "^4.9.5",
    "mongoose-unique-validator": "^1.0.5",
    "validator": "^7.0.0"
  }
}
